<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'Ctrl+C' or 'Ctrl+Insert'</ShortCut>
          <Description>Copy</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug">
        <Item>
          <IdeaActionID>ActivateDebugToolWindow</IdeaActionID>
          <ShortCut>'Alt+5'</ShortCut>
          <Description>Debug</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Debug 'runserver'">
        <Item>
          <IdeaActionID>Debug</IdeaActionID>
          <ShortCut>'Shift+F9'</ShortCut>
          <Description>Debug 'runserver'</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rerun 'runserver'">
        <Item>
          <IdeaActionID>Rerun</IdeaActionID>
          <ShortCut>'Ctrl+F5'</ShortCut>
          <Description>Rerun 'runserver'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Run 'runserver'">
        <Item>
          <IdeaActionID>Run</IdeaActionID>
          <ShortCut>'Shift+F10'</ShortCut>
          <Description>Run 'runserver'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Ctrl+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop 'runserver'">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop 'runserver'</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Stop...">
        <Item>
          <IdeaActionID>Stop</IdeaActionID>
          <ShortCut>'Ctrl+F2'</ShortCut>
          <Description>Stop...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'Ctrl+F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>5</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>